<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/color_white"
    android:elevation="2dp"
    android:paddingTop="@dimen/qb_px_25">


    <com.dresses.library.widget.TypeFaceControlTextView
        android:id="@+id/ivBaseBack"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:minHeight="@dimen/qb_px_18"
        android:layout_marginLeft="@dimen/qb_px_15"
        android:gravity="center"
        android:padding="@dimen/dp_10"
        android:textColor="@color/alibrary_text_color_black"
        android:textSize="@dimen/qb_px_13"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_chainStyle="packed"
        tools:src="@mipmap/ic_empty" />

    <com.dresses.library.widget.TypeFaceControlTextView
        android:id="@+id/tvBaseTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#333333"
        android:textSize="@dimen/qb_px_17"
        app:is_bold="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="Title" />


    <com.dresses.library.widget.TypeFaceControlTextView
        android:id="@+id/btn_first"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/qb_px_8"
        android:gravity="center"
        android:padding="4dp"
        android:textColor="@color/alibrary_up_text_color_gray"
        android:textSize="@dimen/qb_px_13"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/btn_second"
        app:layout_constraintTop_toTopOf="parent" />

    <com.dresses.library.widget.TypeFaceControlTextView
        android:id="@+id/btn_second"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/qb_px_8"
        android:gravity="center"
        android:padding="4dp"
        android:textColor="@color/alibrary_up_text_color_gray"
        android:textSize="@dimen/qb_px_13"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>